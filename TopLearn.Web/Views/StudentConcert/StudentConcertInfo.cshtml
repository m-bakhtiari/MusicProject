            background-repeat: no-repeat;
            background-size: cover;
            border-radius: 254.5px;
            width: 90%;
            height: 90%;
            transition: all .3s;
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
        }

        .product-item:hover:before {
            opacity: .8;
            background-image: url("img/hoverBack.png");
        }

        .productList .swiper-slide {
            opacity: 0;
            visibility: hidden;
            z-index: 10;
            justify-content: center;
            align-items: center;
            width: 100%;
            height: 100%;
            transition: opacity .3s;
            display: flex;
            position: absolute;
            top: 0;
            left: 0;
        }

            .productList .swiper-slide.active {
                opacity: 1;
                visibility: visible;
                height: auto;
                position: relative;
            }

            .productList .swiper-slide img {
                object-fit: contain;
                border-radius: 12px;
                width: auto;
                max-width: 100%;
                height: auto;
                max-height: 100%;
